<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ticktrack.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="TickTrack" textAlignment="CENTER">
    <font>
      <Font name="Segoe Fluent Icons" size="15.0"/>
    </font>
  </Text>

  <Label fx:id="welcomeText"/>
  <Button onAction="#onHelloButtonClick" text="Hello!"/>
</VBox>
